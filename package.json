{
  "name": "portfoliov5",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "lint": "next lint",
    "lint:fix": "eslint --fix --cache --ext .ts,.tsx src",
    "prettier": "prettier --write src",
